{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "include": ["env.d.ts", ".nuxt/nuxt.d.ts", "types/**/*.d.ts", "**/*"]
}
